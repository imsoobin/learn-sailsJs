<div class="my-5 auth-box">
  <h3 class="mb-3">SignUp form</h3>
  <form action="/users/login" method="POST">
    <label for="fname">FirstName:</label><br />
    <input type="text" id="fname" name="firstname" required /><br />
    <label for="lname">LastName:</label><br />
    <input type="text" id="lname" name="lastname" required /><br />
    <label for="fname">Email:</label><br />
    <input type="text" id="fname" name="email" required /><br />
    <label for="lname">Password:</label><br />
    <input type="text" id="lname" name="password" required /><br /><br />
    <input type="submit" value="Submit" name="submit" />
    <a href="/pages/login">Back login</a>
  </form>
</div>
